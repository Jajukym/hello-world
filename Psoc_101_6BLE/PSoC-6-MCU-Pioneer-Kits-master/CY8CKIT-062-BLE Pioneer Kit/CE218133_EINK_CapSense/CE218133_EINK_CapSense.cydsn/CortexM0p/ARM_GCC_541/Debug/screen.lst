 185:screen.c      **** void GoToTextPage(void)
 186:screen.c      **** {
 187:screen.c      ****     /* Variable that stores the index of the character array, which in turn 
 188:screen.c      ****        stores the current text page as a string */
 189:screen.c      ****     uint8_t currentPageIndex;
 190:screen.c      ****     
 191:screen.c      ****     /* Change the current screen type to text page */
 192:screen.c      ****     currentScreen.screen = TEXT_PAGE;
 193:screen.c      **** 
 194:screen.c      ****     /* Re-initialize the text page index to point to the start page */
 195:screen.c      ****     currentScreen.textPage = TEXT_PAGE_INDEX_START;
 196:screen.c      ****     
 197:screen.c      ****     /* Access the index array and fetch the index of the character array that 
 198:screen.c      ****       stores the current text page as a string */
 199:screen.c      ****     currentPageIndex = textPageIndex[currentScreen.menuItem]
 200:screen.c      ****                                     [currentScreen.textPage];
 201:screen.c      ****     
 202:screen.c      ****     /* Check if the fetched index is valid */
 203:screen.c      ****     if (currentPageIndex != INVALID_PAGE_INDEX)
 204:screen.c      ****     {
 205:screen.c      ****         /* Display the text page with background image */
 206:screen.c      ****         DisplayImageAndText((char*) textPage[currentPageIndex],
 207:screen.c      ****                             (image*) textPageBackground);
 208:screen.c      ****     }
 209:screen.c      **** }
